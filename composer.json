{
    "name": "bingogg14/slugify",
    "type": "library",
    "description": "Converts a string into a slug. Port from simov/slugify",
    "keywords": [
        "slug",
        "slugify"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavlo Harashchenko",
            "email": "bingogg14@gmail.com",
            "homepage": "https://p-h-h.com"
        }
    ],
    "require": {
        "php": "^7.1 || ~8.0.0 || ~8.1.0 || ~8.2.0",
        "ext-mbstring": "*",
        "ext-json": "*"
    },
    "conflict": {},
    "require-dev": {
        "laravel/framework": "^5.0|^6.0|^7.0|^8.0",
        "mikey179/vfsstream": "~1.6.8",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Bingogg\\Slugify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bingogg\\Slugify\\Tests\\": "tests"
        }
    }
}
